{
  "env": {
    "node": true,
    "commonjs": true,
    "es6": true,
    "jest": true
  },
  "plugins": [
    "react-hooks"
  ],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "extends": ["airbnb", "standard", "standard-react"],
  "rules": {
    "arrow-parens": ["error", "as-needed"],
    "import/no-cycle": 0,
    "jsx-quotes": ["error", "prefer-single"],
    "no-nested-ternary": 0,
    "no-underscore-dangle": 0,
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0
  }
}
