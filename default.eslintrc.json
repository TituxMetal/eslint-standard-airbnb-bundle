{
  "env": {
    "node": true,
    "commonjs": true,
    "es6": true,
    "jest": true
  },
  "plugins": [
    "react-hooks"
  ],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "extends": [
    "airbnb",
    "standard"
  ],
  "rules": {
    "react/jsx-filename-extension": 0,
    "react/jsx-props-no-spreading": 0,
    "react/prop-types": 0,
    "react/react-in-jsx-scope": 0,
    "no-nested-ternary": 0,
    "arrow-parens": [
      "error",
      "as-needed"
    ],
    "jsx-quotes": [
      "error",
      "prefer-single"
    ]
  }
}
